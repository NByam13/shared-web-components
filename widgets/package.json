        "tailwindcss": "^4.1.8",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^6.0.2",
        "@types/node": "^22.15.24",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vue/tsconfig": "^0.7.0",
        "typescript": "~5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.4",
        "vue-tsc": "^2.2.8"
    }
