{
    "name": "@solanavehikl/shared-web-components",
    "type": "module",
    "version": "8.0.0",
    "description": "A package of sharable components",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NByam13/shared-web-components.git"
    },
    "keywords": [
        "web-components"
    ],
    "author": "solana-vehikl",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/NByam13/shared-web-components/issues"
    },
    "homepage": "https://github.com/NByam13/shared-web-components#readme",
    "files": [
        "dist"
    ],
    "main": "./dist/shared-web-components.js",
    "module": "./dist/shared-web-components.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "types": "./dist/main.d.ts",
